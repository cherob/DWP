		<script>
		
			var yValues = []; 
			var xValues = [];
			var info = "";
			$("#tags").keyup(function(event){
				if(event.keyCode == 13){
					$("#tags").click();
					info = document.getElementById("tags").value;
					$.each(amountTime[info], function( index, value ) {
						console.log(index+" "+value);
												
						addArray(index,value);
					console.log(xValues);
					});
				}
			});
		
			
		
		
		var data = [
			  {
				x:xValues,
				y:yValues,
				type: 'scatter'
			  }
			];

			Plotly.newPlot('myDiv', data);
			
		
		function addArray(date,wert) {
		
			$.each(date, function( index, value ) {
				var total = xValues.push(index);
			});
			
			$.each(wert, function( index, value ) {
				var total = yValues.push(index);
			});
		}
		</script>
  
